{
  "name": "cla-workstation-order",
  "version": "0.1.0",
  "description": "A WordPress plugin for ordering workstations for the Texas A&M College of Liberal Arts.",
  "main": "cla-workstation-order.php",
  "private": true,
  "scripts": {
    "start": "git config core.hooksPath hooks && chmod u+x ./hooks/pre-commit && composer install && npm install && grunt",
    "checkphp": "npm run configphpcs && vendor/bin/phpcs --standard=WordPress ./cla-workstation-order.php ./src ./fields ./templates",
    "fixphp": "npm run configphpcs && vendor/bin/phpcbf --standard=WordPress ./cla-workstation-order.php ./src ./fields ./templates",
    "configphpcs": "vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs"
  },
  "author": "Zachary Watkins",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.tamu.edu/liberalarts-web/cla-workstation-order/issues",
    "email": "zwatkins2@tamu.edu"
  },
  "dependencies": {
    "autoprefixer": "^9.2.1",
    "foundation-sites": "^6.4.3",
    "grunt": "^1.0.3",
    "grunt-contrib-coffee": "^2.0.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^3.0.2",
    "grunt-sass-lint": "^0.2.4",
    "jquery": "^3.3.1",
    "node-sass": "^4.12.0",
    "underscore": "^1.9.1",
    "what-input": "^5.1.2"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.tamu.edu/liberalarts-web/cla-workstation-order.git"
  },
  "homepage": "https://github.tamu.edu/liberalarts-web/cla-workstation-order#readme",
  "browserslist": [
    "last 2 versions",
    "ie > 9"
  ]
}
